<Style name="texto_area">
    <Rule>
      <Filter>[tipo] = 'militar'</Filter>
      <MaxScaleDenominator>200000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Bold" size="9" fill="pink" halo_radius="1" wrap_width="10"/>
    </Rule>
    <Rule>
      <Filter>[area] &gt;= 150000</Filter>
      <MaxScaleDenominator>50000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="10" fill="#000033" halo_radius="1" wrap_width="20" dy="-8"/>
    </Rule>
    <Rule>
      <Filter>[area] &gt;= 30000 and [area] &lt; 150000</Filter>
      <MaxScaleDenominator>20000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="10" fill="#000033" halo_radius="1" wrap_width="20" dy="-8"/>
    </Rule>
    <Rule>
      <Filter>[area] &gt;= 3000 and [area] &lt; 80000</Filter>
      <MaxScaleDenominator>10000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="10" fill="#000033" halo_radius="1" wrap_width="20" dy="-8"/>
    </Rule>
    <Rule>
      <Filter>[area] &lt; 3000</Filter>
      <MaxScaleDenominator>5000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="10" fill="#000033" halo_radius="1" wrap_width="20" dy="-8"/>
    </Rule>
</Style>
