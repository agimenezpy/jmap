<Style name="texto_lugar">
    <Rule>
      <Filter>[tipo] = 'pub'</Filter>
      <MaxScaleDenominator>5000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Bold" size="10" fill="brown" dy="16" halo_radius="1" wrap_width="0"/>
    </Rule>
     <Rule>
      <Filter>[tipo].match('templo.*') or [tipo] = 'comisaria'</Filter>
      <MaxScaleDenominator>10000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="10" fill="#000033" dy="18" halo_radius="1" wrap_width="30"/>
    </Rule>
    <Rule>
      <Filter>[tipo] = 'cerro'</Filter>
      <MaxScaleDenominator>100000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="10" fill="brown" dy="10" halo_radius="1" wrap_width="20"/>
    </Rule>
    <Rule>
      <Filter>[tipo] = 'agua' or [tipo] = 'lago' or [tipo] = 'reserva'</Filter>
      <MaxScaleDenominator>20000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="10" fill="#6699cc" halo_radius="1" wrap_width="20"/>
    </Rule>
    <Rule>
      <Filter>[tipo] = 'escuela' or [tipo] = 'colegio' or [tipo] = 'universidad'</Filter>
      <MaxScaleDenominator>10000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="8" fill="#000033" halo_radius="2" wrap_width="12"/>
    </Rule>
    <Rule>
      <Filter>[tipo] = 'hospital'</Filter>
      <MaxScaleDenominator>10000</MaxScaleDenominator>
      <TextSymbolizer name="nombre" face_name="DejaVu Sans Book" size="9" fill="#000033" dy="12" halo_radius="2" wrap_width="12"/>
    </Rule>
</Style>